<?php

namespace Database\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Uoms
 *
 * @ORM\Table(name="Uoms")
 * @ORM\Entity
 */
class Uoms
{
    /**
     * @var integer
     *
     * @ORM\Column(name="uom_id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $uomId;

    /**
     * @var string
     *
     * @ORM\Column(name="uom_name", type="string", length=45, nullable=false)
     */
    private $uomName;

    /**
     * @var string
     *
     * @ORM\Column(name="record_status", type="string", length=45, nullable=false)
     */
    private $recordStatus = 'Active';


}
